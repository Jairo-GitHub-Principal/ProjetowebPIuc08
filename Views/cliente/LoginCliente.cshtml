@model cliente

 
@{
    Layout = "_Layout"; // chama algumas configuraçoes padões de layout para se aplicado nessa pagina, ao estilos e elementos estão definidos na pagina compartilhada "_Layout.cshtml" 
}



 <div class="row pt-5" >
            <div class="col-md-3 col-lg-4"></div>
                <div class="col-sm-12 col-md-6 col-lg-4 text-center mb-3 mt-5 text-light">
                    <h5>Acesse sua conta aqui</h5>
                </div>
                <div class="col-md-3 col-lg-4"></div>
            </div>
       
        

            <div class="row pb-5 pt-5 mb-5 mt-5 " >
            <div class="col-md-3 col-lg-4"></div>
                <div class="col-sm-12 col-md-6 col-lg-4 text-center">
            <form method="post" asp-controller="cliente" asp-action="LoginCliente">
  <div class="form-group">
   
    <input asp-for="nomeUsuari" type="text" class="form-control"   placeholder="Seu email" value="">
    
  </div>
  <div class="form-group">
   
    <input asp-for="senha" type="password" class="form-control"  placeholder="Senha" value="">
  </div>
  
  <button type="submit" class="btn btn-primary">Enviar</button>
</form>
            

<div class="p-5 mb-5 mt-5 ">
     <h3><a class="nav-link text-warning" asp-area="" asp-controller="admin" asp-action="adminlogin">Admin Login</a></h3>
</div>


<h4>@ViewBag.msg</h4>
</div>
                <div class="col-md-3 col-lg-4 " ></div>
            </div>
            

       
       

    



